DEPS_20 = $(DEPS_20) $(CONFIG)\obj\pcre_compile.obj
DEPS_20 = $(DEPS_20) $(CONFIG)\obj\pcre_exec.obj
DEPS_20 = $(DEPS_20) $(CONFIG)\obj\pcre_globals.obj
