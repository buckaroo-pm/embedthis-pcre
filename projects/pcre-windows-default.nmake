$(CONFIG)\inc\config.h: $(DEPS_2)
	@echo '      [Copy] $(CONFIG)/inc/config.h'
	if not exist "$(CONFIG)\inc" md "$(CONFIG)\inc"
	copy /Y src\config.h $(CONFIG)\inc\config.h

